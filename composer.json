{
    "name": "projektgopher/laravel-blog",
    "description": "Lowest barrier to entry blogging solution for Laravel",
    "keywords": [
        "ProjektGopher",
        "laravel",
        "laravel-blog",
        "blog"
    ],
    "homepage": "https://github.com/projektgopher/laravel-blog",
    "license": "MIT",
    "authors": [
        {
            "name": "Len Woodward",
            "email": "Len@ProjektGopher.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Projektgopher\\Blog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Projektgopher\\Blog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Projektgopher\\Blog\\BlogServiceProvider"
            ],
            "aliases": {
                "Blog": "Projektgopher\\Blog\\BlogFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
